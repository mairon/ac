<p id="notice"><%= notice %></p>

<h1>Listing Schedules</h1>

<table>
  <thead>
    <tr>
      <th>In</th>
      <th>To</th>
      <th>Weekday</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @schedules.each do |schedule| %>
      <tr>
        <td><%= schedule.in %></td>
        <td><%= schedule.to %></td>
        <td><%= schedule.weekday %></td>
        <td><%= link_to 'Destroy', [:admin, schedule], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
